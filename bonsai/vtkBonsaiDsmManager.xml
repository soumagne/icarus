<ServerManagerConfiguration>

  <!-- ================================================================ -->
  <!-- DSM classes wrapped by server manager                            -->
  <!-- ================================================================ -->

  <ProxyGroup name="icarus_helpers">

    <!-- ============================================================== -->
    <!-- AbstractDsmManager                                                     -->
    <!-- ============================================================== -->
    <Proxy name="BonsaiDsmManager" class="vtkBonsaiDsmManager"
        base_proxyname="AbstractDsmManager"
        base_proxygroup="icarus_helpers">

      <IntVectorProperty
        name="QuickSync"
        command="SetQuickSync"
        label="QuickSync"
        number_of_elements="1"
        default_values="1">
      </IntVectorProperty>

      <IntVectorProperty
        name="SampleRate"
        label="SampleRate"
        command="SetSampleRate"
        number_of_elements="1"
        default_values="100">
      </IntVectorProperty>
      
      <IntVectorProperty
        name="Listening"
        label="Listening"
        command="SetListening"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>
          
    </Proxy>
  </ProxyGroup>


</ServerManagerConfiguration>
