<ServerManagerConfiguration>

  <!-- ================================================================ -->
  <!-- DSM classes wrapped by server manager                            -->
  <!-- ================================================================ -->

  <ProxyGroup name="icarus_helpers">
    
    <!-- ============================================================== -->
    <!-- DSMManager                                                     -->
    <!-- ============================================================== -->
    <Proxy name="DSMManager" class="vtkDSMManager">
      
      <Property name="CreateDSM"
                command="CreateDSM">
      </Property>      
      
      <Property name="DestroyDSM"
                command="DestroyDSM">
      </Property>
      
      <Property name="ClearDSM"
                command="ClearDSM">
      </Property>
      
      <Property name="ConnectDSM"
                command="ConnectDSM">
      </Property>
      
      <Property name="DisconnectDSM"
                command="DisconnectDSM">
      </Property>
      
      <Property name="PublishDSM"
                command="PublishDSM">
      </Property>

      <Property name="UnpublishDSM"
                command="UnpublishDSM">
      </Property>

      <Property name="ReadDSMConfigFile"
                command="ReadDSMConfigFile">
      </Property>

      <Property name="H5Dump"
                command="H5Dump">
      </Property>
      
      <Property name="H5DumpLight"
                command="H5DumpLight">
      </Property>
      
      <Property name="H5DumpXML"
                command="H5DumpXML">
      </Property>
      
      <Property name="GenerateXMFDescription"
                command="GenerateXMFDescription">
      </Property>
      
      <Property name="SendDSMXML"
                command="SendDSMXML">
      </Property>
      
      <Property name="RequestRemoteChannel"
                command="RequestRemoteChannel">
      </Property>

      <IntVectorProperty
        name="DsmIsServer"
        command="SetDsmIsServer"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>
      
      <IntVectorProperty
        name="DsmCommType"
        command="SetDsmCommType"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>
      
      <IntVectorProperty
        name="DsmLocalBufferSize"
        command="SetLocalBufferSizeMBytes"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>
      
      <StringVectorProperty
        name="PublishedServerHostName"
        command="GetServerHostName"
        information_only="1"
        number_of_elements="1">
        <SimpleStringInformationHelper/>
      </StringVectorProperty>
      
      <IntVectorProperty
        name="PublishedServerPort"
        command="GetServerPort"
        information_only="1"
        number_of_elements="1"
        default_values="0">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>
      
      <StringVectorProperty
        name="ServerHostName"
        command="SetServerHostName"
        number_of_elements="1">
      </StringVectorProperty>
      
      <IntVectorProperty
        name="ServerPort"
        command="SetServerPort"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>
            
      <StringVectorProperty
        name="XMFDescriptionFilePath"
        command="SetXMFDescriptionFilePath"
        number_of_elements="1">
      </StringVectorProperty>

      <StringVectorProperty
        name="HelperProxyXMLString"
        command="SetHelperProxyXMLString"
        number_of_elements="1">
      </StringVectorProperty>      

      <IntVectorProperty
        name="AcceptedConnection"
        command="GetAcceptedConnection"
        information_only="1"
        number_of_elements="1"
        default_values="0">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <IntVectorProperty
        name="DsmUpdateReady"
        command="GetDsmUpdateReady"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <Property name="ClearDsmUpdateReady"
                command="ClearDsmUpdateReady">
      </Property>

      <IntVectorProperty
        name="DsmDataIsModified"
        command="GetDsmDataIsModified"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <Property name="ClearDsmDataIsModified"
                command="ClearDsmDataIsModified">
      </Property>

      <IntVectorProperty
        name="DsmUpdateLevel"
        command="GetDsmUpdateLevel"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <Property name="ClearDsmUpdateLevel"
                command="ClearDsmUpdateLevel">
      </Property>

      <IntVectorProperty
        name="InteractionsGroupPresent"
        command="GetInteractionsGroupPresent"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>


      <StringVectorProperty
        name="SteeringCommand"
        command="SetSteeringCommand"
        number_of_elements="1">
      </StringVectorProperty>

      <StringVectorProperty
        name="DisabledObject"
        command="SetDisabledObject"
        number_of_elements="1">
      </StringVectorProperty>

    </Proxy>

  </ProxyGroup>


</ServerManagerConfiguration>
