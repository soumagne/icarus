<ServerManagerConfiguration>

  <!-- ================================================================ -->
  <!-- DSM classes wrapped by server manager                            -->
  <!-- ================================================================ -->

  <ProxyGroup name="icarus_helpers">

    <!-- ============================================================== -->
    <!-- DsmManager                                                     -->
    <!-- ============================================================== -->
    <Proxy name="DsmManager" class="vtkDsmManager">

      <Property name="Create"
                command="Create">
      </Property>      

      <Property name="Destroy"
                command="Destroy">
      </Property>

      <Property name="ClearStorage"
                command="ClearStorage">
      </Property>

      <Property name="Connect"
                command="Connect">
      </Property>

      <Property name="Disconnect"
                command="Disconnect">
      </Property>

      <Property name="Publish"
                command="Publish">
      </Property>

      <Property name="Unpublish"
                command="Unpublish">
      </Property>

      <Property name="ReadConfigFile"
                command="ReadConfigFile">
      </Property>

      <Property name="H5Dump"
                command="H5Dump">
      </Property>

      <Property name="H5DumpLight"
                command="H5DumpLight">
      </Property>

      <Property name="H5DumpXML"
                command="H5DumpXML">
      </Property>

      <Property name="GenerateXMFDescription"
                command="GenerateXMFDescription">
      </Property>

      <Property name="SendDSMXML"
                command="SendDSMXML">
      </Property>

      <IntVectorProperty
        name="IsServer"
        command="SetIsServer"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>

      <IntVectorProperty
        name="InterCommType"
        command="SetInterCommType"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>

      <IntVectorProperty
        name="UseStaticInterComm"
        command="SetUseStaticInterComm"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>

      <IntVectorProperty
        name="LocalBufferSizeMBytes"
        command="SetLocalBufferSizeMBytes"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>

      <StringVectorProperty
        name="PublishedServerHostName"
        command="GetServerHostName"
        information_only="1"
        number_of_elements="1">
        <SimpleStringInformationHelper/>
      </StringVectorProperty>

      <IntVectorProperty
        name="PublishedServerPort"
        command="GetServerPort"
        information_only="1"
        number_of_elements="1"
        default_values="0">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <StringVectorProperty
        name="ServerHostName"
        command="SetServerHostName"
        number_of_elements="1">
      </StringVectorProperty>

      <IntVectorProperty
        name="ServerPort"
        command="SetServerPort"
        number_of_elements="1"
        default_values="0">
      </IntVectorProperty>

      <StringVectorProperty
        name="XMFDescriptionFilePath"
        command="SetXMFDescriptionFilePath"
        number_of_elements="1">
      </StringVectorProperty>

      <StringVectorProperty
        name="HelperProxyXMLString"
        command="SetHelperProxyXMLString"
        number_of_elements="1">
      </StringVectorProperty>      

      <IntVectorProperty
        name="IsConnected"
        command="GetIsConnected"
        information_only="1"
        number_of_elements="1"
        default_values="0">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <IntVectorProperty
        name="Notification"
        command="GetNotification"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <IntVectorProperty
        name="IsDataModified"
        command="GetIsDataModified"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <Property name="ClearIsDataModified"
                command="ClearIsDataModified">
      </Property>

      <Property name="SignalUpdated"
                command="SignalUpdated">
      </Property>

      <IntVectorProperty
        name="InteractionsGroupPresent"
        command="GetInteractionsGroupPresent"
        default_values="0"
        information_only="1"
        number_of_elements="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <StringVectorProperty
        name="SteeringCommand"
        command="SetSteeringCommand"
        number_of_elements="1">
      </StringVectorProperty>

      <StringVectorProperty
        name="DisabledObject"
        command="SetDisabledObject"
        number_of_elements="1">
      </StringVectorProperty>

      <Property name="UpdateSteeredObjects"
                command="UpdateSteeredObjects">
      </Property>

    </Proxy>

  </ProxyGroup>


</ServerManagerConfiguration>
